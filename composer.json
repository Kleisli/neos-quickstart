{
    "name": "kleisli/neos-quickstart",
    "type": "project",
    "description": "Neos Quickstart Distribution",
    "license": "GPL-3.0-or-later",
    "support": {
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "neos/neos": "^9.0",
        "neos/contentgraph-doctrinedbaladapter": "^9.0",
        "neos/neos-ui": "^9.0",
        "neos/seo": "~4.1",
        "shel/neos-logs": "^1.4",
        "neos/redirecthandler": "^6.0",
        "neos/redirecthandler-databasestorage": "*",
        "neos/redirecthandler-neosadapter": "^5.0",
        "neos/redirecthandler-ui": "^3.0",
        "kleisli/kubbar" : "dev-main",
        "quickstart/site" : "@dev"
    },
    "require-dev": {
        "neos/buildessentials": "^9.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^4.0",
        "neos/neos-setup": "^4.0",
        "neos/site-kickstarter": "^9.0",
        "shel/nodetypes-analyzer": "^2.1",
        "carbon/pipeline": "^0.18",
        "sitegeist/magicwand": "^4.5",
        "sitegeist/chantalle": "^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./DistributionPackages/*"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Kleisli/Kubbar"
        }
    ],
    "allow-plugins": {
        "neos/composer-plugin": true,
        "composer/package-versions-deprecated": true
    },
    "scripts": {
        "post-create-project-cmd": "./flow welcome",
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
